@model IEnumerable<Schedule.Models.Schedule>

<h1>Schedule</h1>

<table border="1">
    <thead>
        <tr>
            <th>Day</th>
            <th>Class</th>
            <th>Subject</th>
            <th>Teacher</th>
            <th>Start Time</th>
            <th>End Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model)
        {
            <tr>
                <td>@schedule.DayOfWeek</td>
                <td>@schedule.ClassName</td>
                <td>@schedule.SubjectName</td>
                <td>@schedule.TeacherName</td>
                <td>@schedule.StartTime</td>
                <td>@schedule.EndTime</td>
            </tr>
        }
    </tbody>
</table>
